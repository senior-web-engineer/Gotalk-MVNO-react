<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:hdr="http://www.plintron.com/hdr/">
   <soapenv:Header>
      <hdr:COMMUNICATION_METHOD>0</hdr:COMMUNICATION_METHOD>
      <hdr:CHANNEL_REFERENCE>CARE</hdr:CHANNEL_REFERENCE>
      <hdr:ENTITY><%= entity %></hdr:ENTITY>
      <hdr:TRANSACTION_ID><%= transaction_id %></hdr:TRANSACTION_ID>
   </soapenv:Header>
   <soapenv:Body>
       <PORTOUT_VALIDATION_REQUEST>
           <MSISDN><%= msisdn %></MSISDN>
           <OSP_ACCOUNT_NUMBER><%= osp_account_number %></OSP_ACCOUNT_NUMBER>
           <OSP_ACCOUNT_PASSWORD><%= osp_account_password %></OSP_ACCOUNT_PASSWORD>
           <NAME><%= name %></NAME>
           <ADDRESS_LINE1><%= address_line %></ADDRESS_LINE1>
           <CITY><%= city %></CITY>
           <STATE><%= state %></STATE>
           <ZIP><%= zip %></ZIP>
           <LAST_NAME><%= last_name %></LAST_NAME>
       </PORTOUT_VALIDATION_REQUEST>
   </soapenv:Body>
</soapenv:Envelope>